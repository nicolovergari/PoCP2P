<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/insertauto"
        tools:context=".InsertAuto">

    <TextView
            android:text="Inserimento auto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/textView10"
    />
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/text_marca"
            android:hint="Marca"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Modello"
            android:ems="10"
            android:id="@+id/text_modello"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Targa"
            android:ems="10"
            android:id="@+id/text_targa"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Km percorsi"
            android:ems="10"
            android:id="@+id/editText10"/>
    <Button
            android:text="Date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/button5"/>
    <TextView
            android:hint="Date disponibili"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/textView12"/>
    <Button
            android:text="Orari"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/button6"/>
    <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/textView13"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Zona"
            android:ems="10"
            android:id="@+id/editText12"/>
    <Button
            android:text="CONFERMA"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/bt_conferma"/>
</LinearLayout>